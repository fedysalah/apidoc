{
    "name": "react-express-client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
      "react": "16.12.0",
      "react-dom": "16.12.0"
    },
    "devDependencies": {
      "@babel/core": "^7.0.0-0",
      "@babel/plugin-proposal-class-properties": "^7.4.0",
      "@babel/plugin-transform-runtime": "^7.4.0",
      "@babel/preset-env": "7.2.3",
      "@babel/preset-react": "7.0.0",
      "parcel-bundler": "1.12.4",
      "prettier": "1.15.2"
    },
    "babel": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "entry"
          }
        ]
      ],
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "corejs": false,
            "helpers": true,
            "regenerator": true,
            "useESModules": false
          }
        ],
        [
          "@babel/plugin-proposal-class-properties",
          {
            "loose": false
          },
          "some fucking name"
        ]
      ]
    },
    "scripts": {
      "start": "parcel serve --port 3001 src/index.js --global JS",
      "build": "parcel build src/index.js --out-dir ../public/bundle --global JS"
    }
  }
  